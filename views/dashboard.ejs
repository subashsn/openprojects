										<a href="<%=notifications[i].link%>">

											<span class="notification-message"><%= notifications[i].message%></span>
											<time class="timeago pull-right m-r text-muted" datetime="<%=notifications[i].timestamp%>"></time>
										</a>
									</li>
								<% } %>
								</ul>
							</li>
						</ul>
					</li>

					<!--User Profile Section-->
